<div class="container">
  <div class="row">
    <div class="col-12 col-lg-6">
      <div class="h2">Events Created By You:</div>
      <% @by_me_events.each do |event| %>
        <%= render partial: 'share/eventcard', locals: { obj: event } %>
      <% end %>
    </div>
    <div class="col-12 col-lg-6">
      <div class="h2">Events You're Attending:</div>
      <%= render partial: 'share/my-event', locals: { object: @my_events } %>
    </div>
  </div>
</div>

